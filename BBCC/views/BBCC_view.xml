                            <field name="c_name_hijo2" string="Nombre hijo(a)"/>
                            <field name="c_cumple_hijo2" string="Cumpleaños hijo(a)"/>
                            <field name="c_restaurante"/>
                        </group>
                    </page>
                </notebook>
            </field>
        </record>
        
